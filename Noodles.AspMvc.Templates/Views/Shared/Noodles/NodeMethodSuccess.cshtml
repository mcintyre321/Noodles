@using System.Web.Mvc.Html
@using FormFactory
@model Noodles.RequestHandling.ResultTypes.InvokeSuccessResult
<div class="result alert alert-success">
    <a class="close" data-dismiss="alert">×</a>
    <span class="message">Your request has been processed</span>
    @if (Model.Invokeable != null)
    {
        var bestViewName = Html.BestViewName(Model.Invokeable.GetType(), prefix: "Success.");
        if (bestViewName != null)
        {
            <div class="result-content">
                @Html.Partial(bestViewName, Model.Invokeable)
            </div>
        }
    }
</div>
@Html.Partial("Noodles/NodeMethod", Model.Invokeable)