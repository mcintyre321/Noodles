@using System.Web.Mvc.Html
@using FormFactory
@model object
<div class="row">
    <div class="span4">@Html.Partial("Noodles/NodeMethods", Model)&nbsp;</div>
    <div class="span12">
        @Html.Partial("FormFactory/Form.Start", new FormVm(Html))
        @{ Html.RenderPropertiesFor(Model); }
        @Html.Partial("FormFactory/Form.Close", new FormVm(Html))
    </div>
</div>
